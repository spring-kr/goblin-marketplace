{
  "functions": {
    "api/index.py": {
      "runtime": "@vercel/python@4.1.0"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.py"
    }
  ],
  "env": {
    "PYTHONPATH": ".",
    "FLASK_ENV": "production",
    "VERCEL_ENV": "production",
    "LIGHTWEIGHT_MODE": "true"
  }
}
