{
  "functions": {
    "app.py": {
      "runtime": "@vercel/python",
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "env": {
    "PYTHONPATH": ".",
    "FLASK_ENV": "production",
    "VERCEL_ENV": "production",
    "LIGHTWEIGHT_MODE": "true"
  }
}
